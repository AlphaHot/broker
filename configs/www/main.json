{
  "Broker": {
    "Name": "{brokerName}",
    "TCP Port": {
      brokerPort
    },
    "Version": "{brokerVersion}"
  },
  "Log": {
    "Priority": "{brokerLogPriority}",
    "Interactive": {
      brokerLogInteractive
    },
    "Path": "{brokerLogPath}"
  },
  "Net": {
    "Max TCP Clients": {
      brokerNetClients
    }
  },
  "Sessions": {
    "Max Count": {
      brokerSessions
    }
  },
  "Destinations": {
    "Max Count": {
      brokerDestinations
    },
    "Autocreate": "{brokerDestinationAutocreate}"
  },
  "Subscriptions": {
    "Max Count": {
      brokerSubscriptions
    }
  },
  "Threads": {
    "Readers": {
      brokerReaders
    },
    "Writers": {
      brokerWriters
    },
    "Acceptors": {
      brokerAcceptors
    },
    "Subscription-Workers": {
      brokerSubscriptionWorkers
    }
  },
  "DBMS": {
    "Type": "{brokerSDBMS}",
    "Connection Pool": {
      brokerSDBMSPool
    },
    "Connection String": "{brokerSConnection}",
    "DataBase Path": "{brokerSPath}",
    "Journal": {brokerJournal}
  },
  "Persistent Storage": {
    "Payload Path": "{brokerSData}"
  }
}
